{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/config.schema.json",
  "grammars": [
    {
      "name": "gpg",
      "camelcase": "GPG",
      "title": "GPG config",
      "scope": "source.gpg",
      "file-types": [
        "gpg.conf"
      ],
      "injection-regex": "^gpg$",
      "highlights": "queries/highlights.scm",
      "class-name": "TreeSitterGPG"
    }
  ],
  "metadata": {
    "version": "0.3.0",
    "license": "MIT",
    "description": "GPG config grammar for tree-sitter",
    "authors": [
      {
        "name": "ObserverOfTime",
        "email": "chronobserver@disroot.org"
      }
    ],
    "links": {
      "repository": "https://github.com/tree-sitter-grammars/tree-sitter-gpg-config",
      "funding": "https://github.com/sponsors/ObserverOfTime"
    }
  }
}
